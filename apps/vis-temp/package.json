{
  "name": "datavis",
  "description": "Data Vis App",
  "author": "Jeff Walker <walker.jeff.d@gmail.com>",
  "private": true,
  "scripts": {
    "watchify": "cross-env NODE_ENV=development watchify -vd -e src/main.js -o dist/build-dev.js",
    "serve": "http-server -o -s -c 1 -a localhost",
    "dev": "npm-run-all --parallel watchify serve",
    "build": "cross-env NODE_ENV=production browserify -g envify src/main.js | uglifyjs -c warnings=false -m > dist/build.js"
  },
  "dependencies": {
    "d3": "^4.7.4",
    "highcharts": "^5.0.10",
    "vue": "^2.0.1",
    "vue-resource": "^1.3.0",
    "vue-router": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^1.0.6",
    "envify": "^3.4.1",
    "http-server": "^0.9.0",
    "npm-run-all": "^2.1.2",
    "uglify-js": "^2.5.0",
    "vueify": "^9.1.0",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  }
}
